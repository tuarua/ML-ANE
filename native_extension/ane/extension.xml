             <nativeLibrary>libMLANE.a</nativeLibrary>
             <initializer>TRCMLExtInizer</initializer>
             <finalizer>TRCMLExtFinizer</finalizer>
         </applicationDeployment>
     </platform>
     <platform name="appleTV-ARM">
         <applicationDeployment>
             <nativeLibrary>libMLANE.a</nativeLibrary>
             <initializer>TRCMLExtInizer</initializer>
             <finalizer>TRCMLExtFinizer</finalizer>
         </applicationDeployment>
     </platform>
     <platform name="MacOS-x86-64">
         <applicationDeployment>
             <nativeLibrary>MLANE.framework</nativeLibrary>
             <initializer>TRCMLExtInizer</initializer>
             <finalizer>TRCMLExtFinizer</finalizer>
         </applicationDeployment>
     </platform>
    </platforms>
